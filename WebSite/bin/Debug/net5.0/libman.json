{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "library": "jquery@3.6.0",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "font-awesome@5.15.4",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "axios@0.24.0",
      "destination": "wwwroot/axios/"
    },
    {
      "library": "tilt.js@1.2.1",
      "destination": "wwwroot/lib/tilt.js/"
    },
    {
      "library": "vanilla-tilt@1.7.2",
      "destination": "wwwroot/lib/vanilla-tilt/"
    }
  

]
}